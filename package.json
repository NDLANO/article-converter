    "@types/remarkable": "^2.0.3",
    "@ndla/types-article-api": "^0.0.3",
    "@ndla/types-concept-api": "^0.0.4",
    "typescript": "^4.4.3",
    "babel-jest": "^27.2.1",
    "chalk": "^2.4.2",
    "cross-env": "^5.2.0",
    "eslint": "^7.32.0",
    "eslint-config-ndla": "^2.0.4",
    "jest": "^27.2.1",
    "@emotion/jest": "^11.3.0",
    "ts-jest": "^27.0.5",
    "nock": "^11.9.0",
